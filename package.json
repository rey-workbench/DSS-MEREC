{
  "name": "merec-dss",
  "version": "1.0.0",
  "description": "Implementation of MEREC (Method based on Removal Effects of Criteria) algorithm for calculating criteria weights in multi-criteria decision making systems",
  "type": "commonjs",
  "main": "lib/index.cjs.js",
  "module": "lib/index.esm.js",
  "browser": "lib/index.umd.js",
  "types": "lib/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/index.esm.js",
      "require": "./lib/index.cjs.js",
      "browser": "./lib/index.umd.js",
      "types": "./lib/index.d.ts"
    }
  },
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "build": "rollup -c",
    "build:tsc": "tsc",
    "test": "jest",
    "test:ts": "jest --testPathPattern=src",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "merec",
    "criteria-weights",
    "multi-criteria-decision-making",
    "mcdm",
    "decision-support-system",
    "weighting-method",
    "removal-effects",
    "algorithm"
  ],
  "author": "Reynald Silva",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/jest": "^29.0.0",
    "@types/node": "^20.0.0",
    "jest": "^29.0.0",
    "rollup": "^4.0.0",
    "rollup-plugin-dts": "^6.0.0",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/reysilvaa/dss-merec.git"
  },
  "bugs": {
    "url": "https://github.com/reysilvaa/dss-merec/issues"
  },
  "homepage": "https://github.com/reysilvaa/dss-merec#readme"
}
